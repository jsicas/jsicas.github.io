set.seed(1)
n <- 5000; i <- 1; C <- sqrt(2*exp(1)/pi); X <- numeric(n)
f <- function(x) exp(-x^2/2)/sqrt(2*pi)   # densidade Normal(0,1)
g <- function(x) exp(-abs(x))/2           # densidadenisdade Laplace(0,1)
while (i <= n) {
U <- runif(1)               # gerando U ~ U(0,1)
V1 <- -log(1 - runif(1))    # gerando V1 ~ exp(1)
V2 <- -log(1 - runif(1))    # gerando V2 ~ exp(1)
Y <- V1 - V2                # gerando Laplace(0,1)
if (U <= f(Y)/(C * g(Y))) {X[i] <- Y; i <- i + 1}
}
hist(X, prob=T, breaks=30, xlim=c(-4,4), main='Amostra Normal(0,1)')
curve(dnorm(x, mean=0, sd=1), col='blue', add=T)
